{
	"name": "BBall",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "rm -rf node_modules && ./scripts/clean-build-cache.sh && watchman watch-del-all && rm -rf $TMPDIR/react-*",
		"clean-build": "./scripts/clean-build-cache.sh ",
		"clean-start": "npm run clean && npm run rc-start",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"rc-start": "npm start -- --reset-cache",
		"fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean && npm install",
		"fresh-start": "npm run fresh-install && npm run rc-start",
		"android": "react-native run-android --variant StagingNoTestDebug",
		"ios": "react-native run-ios --simulator=\"iPhone 6\"",
		"debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'"
	},
	"dependencies": {
		"@shoutem/ui": "0.23.4",
		"axios": "0.18.0",
		"eslint": "5.0.1",
		"eslint-config-airbnb": "17.0.0",
		"eslint-plugin-import": "2.13.0",
		"eslint-plugin-jsx-a11y": "6.1.0",
		"eslint-watch": "4.0.0",
		"moment": "2.22.2",
		"prop-types": "15.6.2",
		"react": "16.3.0",
		"react-native": "0.55.0",
		"react-native-config": "0.11.5",
		"react-native-device-info": "0.21.5",
		"react-native-navigation": "1.1.474",
		"react-native-vector-icons": "4.6.0",
		"react-redux": "5.0.7",
		"redux": "4.0.0",
		"redux-persist": "4.4.0",
		"redux-thunk": "2.3.0",
		"seamless-immutable": "7.1.3"
	},
	"devDependencies": {
		"@debitoor/eslint-config-debitoor": "2.4.0",
		"babel-cli": "6.26.0",
		"babel-core": "6.26.3",
		"babel-eslint": "8.2.5",
		"babel-jest": "23.2.0",
		"babel-preset-es2015": "6.24.1",
		"babel-preset-es2015-mod": "6.6.0",
		"babel-preset-react": "6.24.1",
		"babel-preset-react-native": "4.0.0",
		"babel-preset-react-native-stage-0": "1.0.1",
		"eslint-plugin-jest": "21.17.0",
		"eslint-plugin-no-only-tests": "2.0.1",
		"eslint-plugin-react": "7.10.0",
		"eslint-plugin-react-native": "3.2.1",
		"jest": "23.3.0",
		"react-native-dotenv": "0.1.1",
		"react-test-renderer": "16.4.1",
		"redux-devtools-extension": "2.13.5",
		"redux-immutable-state-invariant": "2.1.0",
		"remote-redux-devtools": "0.5.12"
	},
	"jest": {
		"preset": "react-native"
	}
}
