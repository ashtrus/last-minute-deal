{
	"name": "LastMinuteDeal",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android --variant StagingNoTestDebug",
		"clean-build": "./scripts/clean-build-cache.sh ",
		"clean": "rm -rf node_modules && ./scripts/clean-build-cache.sh && watchman watch-del-all && rm -rf $TMPDIR/react-*",
		"debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
		"ios": "react-native run-ios --simulator=\"iPhone 6\"",
		"lint": "eslint src",
		"postinstall": "patch-package",
		"precommit": "lint-staged",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest"
	},
	"dependencies": {
		"@shoutem/ui": "0.23.4",
		"axios": "0.18.0",
		"moment": "2.23.0",
		"native-base": "2.9.3",
		"prop-types": "15.6.2",
		"react-native-config": "0.11.7",
		"react-native-device-info": "0.24.3",
		"react-native-navigation": "1.1.474",
		"react-native-vector-icons": "6.1.0",
		"react-native": "0.57.8",
		"react-redux": "5.0.7",
		"react": "16.6.3",
		"redux-persist": "4.4.0",
		"redux-thunk": "2.3.0",
		"redux": "4.0.0",
		"seamless-immutable": "7.1.4"
	},
	"devDependencies": {
		"@babel/cli": "7.2.3",
		"@babel/core": "7.2.2",
		"@babel/preset-env": "7.2.3",
		"@babel/preset-react": "7.0.0",
		"@debitoor/eslint-config-debitoor": "3.0.0",
		"babel-eslint": "10.0.1",
		"babel-jest": "23.6.0",
		"eslint": "5.10.0",
		"eslint-config-prettier": "3.3.0",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jest": "22.1.2",
		"eslint-plugin-jsx-a11y": "6.1.2",
		"eslint-plugin-no-only-tests": "2.1.0",
		"eslint-plugin-prettier": "3.0.0",
		"eslint-plugin-react": "7.11.1",
		"eslint-plugin-react-native": "3.5.0",
		"eslint-watch": "4.0.2",
		"husky": "1.2.1",
		"jest": "23.6.0",
		"lint-staged": "8.1.0",
		"metro-react-native-babel-preset": "0.51.1",
		"patch-package": "5.1.1",
		"prettier": "1.15.3",
		"react-native-dotenv": "0.2.0",
		"react-test-renderer": "16.6.3",
		"redux-devtools-extension": "2.13.7",
		"redux-immutable-state-invariant": "2.1.0",
		"remote-redux-devtools": "0.5.14"
	},
	"jest": {
		"preset": "react-native"
	},
	"lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": ["eslint --fix src", "git add"]
  }
}
